{% extends 'base.html.twig' %}

{% block title %}Ranking{% endblock %}

{% block body %}
    <h1>{{ level }}</h1>
    <table class="table">
            <tr>
                <th>Id</th>
                <th>Barcode</th>
                <th>Shortname</th>
                <th>Lastname</th>
                <th>Mas</th>
                <th>Gender</th>
                <th>grade Id</th>
            </tr>

    {% for lists in list %}
    <tr>
                <td>{{ lists.id }}</td>
                <td>{{ lists.barcode }}</td>
                <td>{{ lists.shortname }}</td>
                <td>{{ lists.lastname }}</td>
                <td>{{ lists.mas }}</td>
                <td>{{ lists.gender }}</td>
                <td>{{ lists.grade.level}}</td>
            </tr>
        {% endfor %}

    <a href="{{ path('app_run_index') }}">back to list</a>

{% endblock %}