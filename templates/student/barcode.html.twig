{% extends 'base.html.twig' %}

{% block title %}Compute time{% endblock %}

{% block body %}

<table>
        {% for student in students %}
        <tr>
                <td>{{ student.id }}</td>
                <td>{{ student.barcode }}</td>
                <td>{{ student.shortname }}</td>
                <td>{{ student.lastname }}</td>
                <td>{{ student.mas }}</td>
                <td>{{ student.gender }}</td>
        </tr>
        {% endfor %}
</table>


{% endblock %}